<%= render 'shared/content_title', title: "Add Category" %>

<%= render 'shared/errors', model_obj: @category %>

<div class='well'>
  <%= form_for @category do |f| %>
    <div class='control-group'>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <br>
    <%= f.submit class: 'btn btn-primary'%>
  <% end %>
</div>

